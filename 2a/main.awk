#!/usr/bin/awk -f

BEGIN {
    sum=0;
}

$1 == "A" && $2 == "X" {
    sum += 3;
}

$1 == "A" && $2 == "Y" {
    sum += 4;
}

$1 == "A" && $2 == "Z" {
    sum += 8;
}

$1 == "B" && $2 == "X" {
    sum += 1;
}

$1 == "B" && $2 == "Y" {
    sum += 5;
}

$1 == "B" && $2 == "Z" {
    sum += 9;
}

$1 == "C" && $2 == "X" {
    sum += 2;
}

$1 == "C" && $2 == "Y" {
    sum += 6;
}

$1 == "C" && $2 == "Z" {
    sum += 7;
}

END {
    print "Total score: ", sum;
}
